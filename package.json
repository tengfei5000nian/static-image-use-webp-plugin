  "main": "dist/static-image-use-webp-plugin.js",
  "types": "dist/static-image-use-webp-plugin.d.ts",
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "webpack",
    "plugin",
    "webp",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tengfei5000nian/static-image-use-webp-plugin.git"
  },
  "peerDependencies": {
    "webpack": ">=5.0.0 <6.0.0"
  },
  "scripts": {
    "clean": "del-cli --dot=true \"./dist/**/*\"",
    "build": "npm run clean && npm run build.compile && npm run build.types",
    "build.compile": "cross-env NODE_ENV=production babel src -d dist --verbose --extensions .ts,.js --ignore */**/*.d.ts,**/**/*.test.ts --source-maps",
    "build.types": "cross-env NODE_ENV=production tsc --project tsconfig.types.json",
    "dev": "npm run clean && cross-env NODE_ENV=development babel -w src -d dist --verbose --extensions .ts,.js --ignore */**/*.d.ts,**/**/*.test.ts --source-maps"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "read-pkg-up": "^6.0.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@types/webpack": "^5.28.0",
    "@types/webpack-sources": "^3.2.0",
    "imagemin-webp": "^7.0.0"
  }
}
